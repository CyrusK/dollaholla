<script type="text/javascript">
  var incentru = incentru || {};
  incentru.userData = incentru.userData || {};
  incentru.userData.isSignedIn = <%= !current_user.nil? %>;
  <% if current_user %>
    incentru.userData.email = "<%= current_user.email %>";
    incentru.userData.id = <%= current_user.id %>;
  <% end %>
</script>
